{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=staging-db-server;Database=Algora_Staging;User Id=algora_staging;Password=#{DB_PASSWORD}#;TrustServerCertificate=True;MultipleActiveResultSets=true"
  },
  "Shopify": {
    "ApiKey": "#{SHOPIFY_API_KEY}#",
    "ApiSecret": "#{SHOPIFY_API_SECRET}#",
    "AppUrl": "https://staging.algora.example.com",
    "Scopes": "read_products,write_products,read_orders,write_orders,read_customers,write_customers",
    "ShopDomain": "#{SHOPIFY_SHOP_DOMAIN}#"
  }
}